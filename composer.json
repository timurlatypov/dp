{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4|^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "algolia/algoliasearch-client-php": "^2.7",
    "anhskohbo/no-captcha": "^3.2",
    "benjacho/belongs-to-many-field": "^2.0",
    "benjaminhirsch/nova-slug-field": "^1.2",
    "buzz/laravel-google-captcha": "*",
    "davidpiesse/nova-toggle": "^0.0.8",
    "dillingham/nova-ajax-select": "^1.0",
    "eminiarts/nova-permissions": "*",
    "facade/ignition": "^2.3.6",
    "fideloper/proxy": "^4.4",
    "guzzlehttp/guzzle": "*",
    "hardevine/shoppingcart": "*",
    "laravel/framework": "^8.0",
    "laravel/nova": "^3",
    "laravel/scout": "^8.6",
    "laravel/socialite": "*",
    "laravel/tinker": "*",
    "laravel/ui": "^3.0",
    "moneyphp/money": "^4.1",
    "nikaia/nova-rating-field": "master",
    "optimistdigital/nova-sortable": "^2.1",
    "orlyapps/nova-belongsto-depend": "^2.0",
    "saumini/count": "^1.0",
    "sentry/sentry": "^3.1",
    "sentry/sentry-laravel": "^2.3.1",
    "spatie/laravel-feed": "3.2.1",
    "spatie/laravel-responsecache": "^7.4",
    "timothyasp/nova-color-field": "^1.0",
    "voronkovich/sberbank-acquiring-client": "*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "*",
    "filp/whoops": "^2.7",
    "fzaninotto/faker": "*",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "^5.9"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./nova"
    }
  ],
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
    "extra": {
      "laravel": {
        "dont-discover": [
        ]
      }
    },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform-check": false,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
