{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2.5",
    "ext-curl": "*",
    "ext-json": "*",
    "algolia/algoliasearch-client-php": "^2.7",
    "andrewnovikof/omnipay-sberbank": "^3.2",
    "anhskohbo/no-captcha": "^3.2",
    "benjaminhirsch/nova-slug-field": "^1.2",
    "buzz/laravel-google-captcha": "*",
    "fideloper/proxy": "^4.4",
    "guzzlehttp/guzzle": "^6.5",
    "hardevine/shoppingcart": "^2.8",
    "laravel/framework": "^7",
    "laravel/nova": "^3",
    "laravel/scout": "^8",
    "laravel/socialite": "^4.4",
    "laravel/tinker": "^2.4",
    "nikaia/nova-rating-field": "^1.0",
    "php-http/guzzle6-adapter": "^2.0",
    "voronkovich/sberbank-acquiring-client": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.4",
    "filp/whoops": "^2.7",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^3.15"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./nova"
    }
  ],
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
