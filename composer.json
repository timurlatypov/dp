{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "*",
    "algolia/algoliasearch-client-php": "*",
    "andrewnovikof/omnipay-sberbank": "*",
    "anhskohbo/no-captcha": "*",
    "barryvdh/laravel-dompdf": "*",
    "buzz/laravel-google-captcha": "*",
    "fideloper/proxy": "*",
    "hardevine/shoppingcart": "*",
    "guzzlehttp/guzzle": "*",
    "laravel/framework": "*",
    "laravel/scout": "*",
    "laravel/socialite": "*",
    "laravel/tinker": "*",
    "php-http/guzzle6-adapter": "*",
    "voronkovich/sberbank-acquiring-client": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "laravel/nova": "*"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "*",
    "filp/whoops": "*",
    "fzaninotto/faker": "*",
    "mockery/mockery": "*",
    "nunomaduro/collision": "*",
    "phpunit/phpunit": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./nova"
    }
  ],
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
